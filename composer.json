{
    "name": "packt/packt-auth",
    "description": "packt-auth",
    "keywords": [
        "packt",
        "packt-auth"
    ],
    "homepage": "https://github.com/packt/packt-auth",
    "license": "MIT",
    "authors": [
        {
            "name": "Sam Thornton",
            "email": "samt@packt.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "illuminate/contracts": "^8.0",
        "laravel/socialite": "^5.1",
        "socialiteproviders/microsoft-azure": "^4.2",
        "spatie/laravel-package-tools": "^1.1"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.3",
        "spatie/laravel-ray": "^1.9"
    },
    "autoload": {
        "psr-4": {
            "Packt\\PacktAuth\\": "src",
            "Packt\\PacktAuth\\Database\\Factories\\": "database/factories"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Packt\\PacktAuth\\Tests\\": "tests"
        }
    },
    "scripts": {
        "psalm": "vendor/bin/psalm",
        "test": "vendor/bin/phpunit --colors=always",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Packt\\PacktAuth\\PacktAuthServiceProvider"
            ],
            "aliases": {
                "PacktAuth": "Packt\\PacktAuth\\PacktAuthFacade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
